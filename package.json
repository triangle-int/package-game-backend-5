{
  "name": "package-backend-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prisma:dev:deploy": "prisma migrate deploy",
    "db:dev:rm": "docker compose rm dev-db -s -f -v",
    "db:dev:up": "docker compose up dev-db -d",
    "db:dev:restart": "run-script-os",
    "db:dev:restart:win32": "yarn db:dev:rm && yarn db:dev:up && timeout 3 && yarn prisma:dev:deploy",
    "db:dev:restart:default": "yarn db:dev:rm && yarn db:dev:up && sleep 3 && yarn prisma:dev:deploy",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "dotenv -e .env.test yarn db:dev:restart",
    "test:e2e": "jest --no-cache --config ./test/jest-e2e.json --forceExit"
  },
  "dependencies": {
    "@jeremybarbet/nest-iap": "^1.4.0",
    "@mapbox/polyline": "^1.1.1",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^1.3.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.4.5",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/websockets": "^8.4.5",
    "@prisma/client": "5",
    "@tfarras/nestjs-firebase-auth": "^2.0.0",
    "@types/lodash": "^4.14.182",
    "cache-manager": "^4.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "emoji-regex": "^10.1.0",
    "firebase": "^9.8.2",
    "firebase-admin": "^10.2.0",
    "geohex": "^0.0.6",
    "lodash": "^4.17.21",
    "nestjs-firebase": "^8.2.3",
    "ngeohash": "^0.6.3",
    "node-apple-receipt-verify": "^1.12.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "prisma": "5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "rxjs": "^7.2.0",
    "socket.io-client": "^4.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/cache-manager": "^4.0.1",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/mapbox__polyline": "^1.0.2",
    "@types/ngeohash": "^0.6.4",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "pactum": "^3.1.9",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "4.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
